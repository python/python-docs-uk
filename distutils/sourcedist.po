msgstr "``gztar``"

msgid "gzip'ed tar file (:file:`.tar.gz`)"
msgstr "gzip-ований tar-файл (:file:`.tar.gz`)"

msgid "\\(2)"
msgstr "\\(2)"

msgid "``bztar``"
msgstr "``bztar``"

msgid "bzip2'ed tar file (:file:`.tar.bz2`)"
msgstr "bzip2-файл tar (:file:`.tar.bz2`)"

msgid "\\(5)"
msgstr "\\(5)"

msgid "``xztar``"
msgstr "``xztar``"

msgid "xz'ed tar file (:file:`.tar.xz`)"
msgstr "xz-ваний tar-файл (:file:`.tar.xz`)"

msgid "``ztar``"
msgstr "``ztar``"

msgid "compressed tar file (:file:`.tar.Z`)"
msgstr "стиснутий файл tar (:file:`.tar.Z`)"

