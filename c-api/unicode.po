msgstr ":attr:`%llu`"

msgid "unsigned long long"
msgstr "без знака довгий довгий"

msgid "Equivalent to ``printf(\"%llu\")``. [1]_"
msgstr "Еквівалент ``printf(\"%llu\")``. [1]_"

msgid ":attr:`%zd`"
msgstr ":attr:`%zd`"

msgid ":c:type:`\\ Py_ssize_t`"
msgstr ":c:type:`\\ Py_ssize_t`"

