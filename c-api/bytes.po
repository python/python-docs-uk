msgid "``%c``"
msgstr "``%c``"

#: ../../c-api/bytes.rst:69 ../../c-api/bytes.rst:72 ../../c-api/bytes.rst:90
#: ../../c-api/bytes.rst:93
msgid "int"
msgstr "внутр"

#: ../../c-api/bytes.rst:69
msgid "A single byte, represented as a C int."
msgstr "Один байт, представлений як C int."

