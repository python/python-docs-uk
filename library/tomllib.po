#: ../../library/tomllib.rst:78
msgid "Parsing a TOML string::"
msgstr ""

#: ../../library/tomllib.rst:80
msgid ""
"import tomllib\n"
"\n"
"toml_str = \"\"\"\n"
"python-version = \"3.11.0\"\n"
"python-implementation = \"CPython\"\n"
"\"\"\"\n"
"\n"
"data = tomllib.loads(toml_str)"
msgstr ""

